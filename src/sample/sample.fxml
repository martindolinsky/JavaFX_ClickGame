<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <Pane prefHeight="300.0" prefWidth="300.0">
         <children>
            <Button fx:id="btnTopLeft" layoutX="45.0" layoutY="50.0" mnemonicParsing="false" onAction="#clickBtnTopLeft" prefHeight="100" prefWidth="100" text="Button" />
            <Button fx:id="btnTopRight" layoutX="150.0" layoutY="50.0" mnemonicParsing="false" onAction="#clickBtnTopRight" prefHeight="100" prefWidth="100" text="Button" />
			 <Button fx:id="btnBottomLeft" layoutX="45.0" layoutY="155.0" mnemonicParsing="false" onAction="#clickBtnBottomLeft" prefHeight="100" prefWidth="100" text="Button" />
			 <Button fx:id="btnBottomRight" layoutX="150.0" layoutY="155.0" mnemonicParsing="false" onAction="#clickBtnBottomRight" prefHeight="100" prefWidth="100" text="Button" />
         </children></Pane>
   </children>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
</GridPane>
